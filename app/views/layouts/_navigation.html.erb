<div class="nav_menu">
	<ul>
		<% Category.all.each do |category| %>
			<li><%= link_to category.name, category %></li>
		<% end %>
	</ul>
</div>


<div class="sign_in">
	<% if current_user %>
		<%= link_to "Log out", session_path(current_user.id), method: :delete %> </br>
	<% else %>
		<%= link_to "Signin", new_session_path %> or <%= link_to "Signup", new_user_path %> 
	<% end %>
</div>

